{
  "version": 2,
  "buildCommand": "cd client && npm install && npm run build",
  "outputDirectory": "client/dist",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_SUPABASE_URL": "VITE_SUPABASE_URL",
    "VITE_SUPABASE_ANON_KEY": "VITE_SUPABASE_ANON_KEY"
  }
} 